.shan{
	animation: shan 0.5s infinite 0s ease-in-out;
	-webkit-animation: shan 0.5s infinite 0s ease-in-out;
	-o-animation: shan 0.5s infinite 0s ease-in-out;
	-ms-animation: shan 0.5s infinite 0s ease-in-out;
}


@keyframes shan {
0% {
	transform:scale(1,1); 
}
50% {
	transform:scale(1.5,1.5); 
}
100% { 
	transform:scale(1,1);
}
}
@-webkit-keyframes shan {
0% {
	-webkit-transform:scale(1,1); 
}
50% {
	-webkit-transform:scale(1.5,1.5); 
}
100% { 
	-webkit-transform:scale(1,1);
}
}

@-o-keyframes shan {
0% {
	-o-transform:scale(1,1); 
}
50% {
	-o-transform:scale(1.5,1.5); 
}
100% { 
	-o-transform:scale(1,1);
}
}

@-ms-keyframes shan {
0% {
	-ms-transform:scale(1,1); 
}
50% {
	-ms-transform:scale(1.5,1.5); 
}
100% { 
	-ms-transform:scale(1,1);
}
}

