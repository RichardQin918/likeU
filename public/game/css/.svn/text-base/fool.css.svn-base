@charset "utf-8";
/* CSS Document */
*{-webkit-tap-highlight-color:rgba(255,0,0,0);-webkit-tap-highlight-color:transparent;-webkit-touch-callout: none;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,    
form,fieldset,input,textarea,p,blockquote,th,td{padding:0;margin:0;font-family:Helvetica,"Microsoft Yahei","微软雅黑",Arial,sans-serif;} 
body {
	 background-color:#fff;
	-webkit-text-size-adjust: 100%;/*解决Iphone下橱窗字体变大问题*/
	font-size:16px;
    
}

nav, footer, section, header, article, figure {padding: 0;margin: 0;display: block;}
ul,li {list-style: none;}
h1,h2,h3,h4,div,li,p,menu,dl,dt,dd{ font-size:100%; text-align:left;}
h1,h2,h3,h4{ font-weight:normal;}
a{ text-decoration:none;}
a:link {
	-webkit-tap-highlight-color: rgba(0,0,0,0);	/*禁止链接高亮*/
	/*-webkit-touch-callout: none;					禁止链接长按弹出选项菜单*/
}
a:link, a:visited, a:hover, a:active { text-decoration:none;}
img{border:0; height:auto; width:100%; max-width:100%;}

input[type="text"]:not(.selectbox),input[type="number"],input[type="password"] {
	-webkit-user-modify: read-write-plaintext-only;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	outline-style: none; 
	border:none;border:0;	
}

table{
	border:0;margin:0;border-collapse:collapse;border-spacing:0;
}

.fw{
	overflow: auto;
}

.fl{
	float: left;
}

.fr{
	float: right;
}

.pa{
	position: absolute;
}

.pr{
	position: relative;
}

.pf{
	position: fixed;
}

.tc{
	text-align: center;
}

.tr{
	text-align: right;
}

.tl{
	text-align: left;
}


html.findex body,html.findex{
	height: 100%;
	width: 100%;
	background: url(../images/fool/i-bg.jpg) center no-repeat;
	background-size: 100% 100%;
	margin: 0;
	padding: 0;
}

.i-bg-2{
	width: 100%;
	top: 10%;
}

.i-bnt{
	width: 70%;
	left: 15%;
	bottom: 10%;
	background: url(../images/fool/i-bnt.png);
	background-size: auto 100%;
	background-position: left top;
}

.i-bnt:active{
	background: url(../images/fool/i-bnt.png);
	background-size: auto 100%;
	background-position: right top;
}

.f-z{
	width: 70%;
	top: 25%;
	left: 15%;
	/* background: url(../images/fool/tt.jpg); */
	background-size: 100%;
}

.f-z-wrap{
	height: 100%;
}

.f-z-wrap-s{
	background: url(../images/fool/t-bg.png) no-repeat;
	background-size: 100%;
}

.f-z-wrap div.pa{
	width: 9%;
	background: url(../images/fool/n-bg.png) center no-repeat;
	background-size: 100%;
	color: #000;
	text-align: center;
	border-radius: 50%;
	font-size: 14px;
	z-index: 100;
}

.f-z-wrap div.pa-right{
	color: #fff;
}

.f-z-wrap div.pa-wrong{
	animation: foot 0.5s infinite 0s ease-in-out;
	-webkit-animation: foot 0.5s infinite 0s ease-in-out;
	-o-animation: foot 0.5s infinite 0s ease-in-out;
	-ms-animation: foot 0.5s infinite 0s ease-in-out;
}


@keyframes foot {
0% {
	background-image: url(../images/fool/n-bg.png);
}
100% { 
	background-image: url(../images/fool/n-bg-2.png);
}
}
@-webkit-keyframes foot {
0% {
	background-image: url(../images/fool/n-bg.png);
}
100% { 
	background-image: url(../images/fool/n-bg-2.png);
}
}
@-o-keyframes foot {
0% {
	background-image: url(../images/fool/n-bg.png);
}
100% { 
	background-image: url(../images/fool/n-bg-2.png);
}
}
@-ms-keyframes foot {
0% {
	background-image: url(../images/fool/n-bg.png);
}
100% { 
	background-image: url(../images/fool/n-bg-2.png);
}
}


.f-z-wrap img.pa{
	display: none;
	z-index: 90;
}



