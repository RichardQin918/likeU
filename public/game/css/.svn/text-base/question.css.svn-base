}

.index-bnt a{
	display: inline-block;
	width: 30%;
}

.index-bnt a:nth-child(1){
	margin-right: 5%;
}

.index-bnt a:nth-child(2){
	margin-left: 5%;
}

.rule{
	display: none;
}

.rule-img{
	position: absolute;
	width: 100%;
	left: 0;
	top: 50%;
	z-index: 200;
}


/*answer*/
.answer-body,.title{
	width: 100%;
	background: url(../images/question/answer-bg.png) repeat-y;
	background-size: 100% auto;
	padding-top: 10px;
}

.answer-t-center{
	width: 100%;
	background: url(../images/question/answer-t-bg.png) repeat-y;
	background-size: 100% auto;
	position: relative;
}

.answer-t-center:before{
	content: "";
	width: 1em;
	height: 1em;
	position: absolute;
	background: url(../images/question/redia-1.png);
	background-size: 100%;
	top: 0.7em;
	left: 12%;
}

.answer-t-center-3:before{
	content: "";
	width: 1em;
	height: 1em;
	position: absolute;
	background: url(../images/question/check-1.png);
	background-size: 100%;
	top: 0.7em;
	left: 12%;
}



.answer-text{
	font-size: 14px;
	width: 70%;
	padding-left: 18%;
	padding-top: 0.6em;
	line-height: 1.5em;
}


.answer-item,.answer-item-s{
	width: 100%;
	margin-bottom: 10px;
}



.this-item{
	color: #1e9de6;
}


.title h2{
	width: 80%;
	margin: 0 auto;
	font-size: 18px;
	padding-bottom: 1em;
	color: #333;
	line-height: 1.5em;
}

.title h3{
	width: 80%;
	margin: 0 auto;
	font-size: 14px;
	padding-bottom: 1em;
	color: #333;
	line-height: 1.5em;
}

.this-item .answer-t-center:before{
	background-image: url(../images/question/redia-2.png);
}

.this-item .answer-t-center-3:before{
	background-image: url(../images/question/check-2.png);
}

.answer-bnt{
	text-align: center;
	padding: 10px 0;
}

.answer-bnt a{
	width: 30%;
	display: inline-block;
}

.answer-bnt a:nth-child(1){
	margin-right: 5%;
}
.answer-bnt a:nth-child(2){
	margin-left: 5%;
}

.answer-bottom{
	position: relative;
}


.answer-tt{
	padding-top: 0.5em;
}

.answer-bottom span{
	position: absolute;
	font-size: 12px;
	color: #333;
	left: 7%;
	bottom: 1.2em;
}

.answer-input{
	width: 78%;
	margin: 0 auto;
	background: #f7f7f7;
	padding: 0.5em 0;
	border-radius: 5px;
}

.answer-input textarea{
	width: 90%;
	display: block;
	margin: 0 auto;
	height: 8em;
	background: none;
	border: none;
	outline: none;
}

.answer-input input{
	width: 90%;
	display: block;
	margin: 0 auto;
	height: 1.5em;
	line-height: 1.5em;
	background: none;
	border: none;
	outline: none;
}

.erwei{
	width: 60%;
	display: block;
	margin: 0 auto;
}

.answer-tt:before,.this-item .answer-tt:before{
	display: none;
}

.tip,.tip-2{
	display: none;
}

.zz{
	position: fixed;
	z-index: 100;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background: #000;
	opacity: 0.5;
}

.tip-container{
	position: fixed;
	z-index: 101;
	width: 60%;
	background: #fff;
	top: 50%;
	margin-top: -4em;
	left: 20%;
	border-radius: 5px;
}

.tip-container h2{
	text-align: center;
	font-size: 16px;
	color: #2fb2fe;
	line-height: 2em;
	margin: 0 10px;
	border-bottom: 1px solid #efefef;
}

.tip-container p{
	text-align: center;
	font-size: 14px;
	text-align: center;
	line-height: 1.5em;
	padding: 1em 2em;
	color: #666;
}

.tip-container .bnt{
	line-height: 3em;
	font-size: 14px;
	color: #333;
	text-align: center;
	border-top: 1px solid #efefef;
}

.tip-container .bnt a{
	color: #ff6b43;
}

.tip-2 .tip-container .bnt a{
	color: #ff6b43;
	display: inline-block;
	width: 48%;
	text-align: center;
}

.tip-2 .tip-container .bnt a:nth-child(1){
	color: #ccc;
}

/*end*/
html.end body,html.share body{
	min-height: 100%;
	background: url(../images/question/index-bg.jpg) repeat-y;
	background-size:100% auto;
}


a.share{
	animation: sharebnt 1s 0s 10000 ease-in forwards;
	-webkit-animation: sharebnt 1s 0s 10000 ease-in forwards;
	-o-animation: sharebnt 1s 0s 10000 ease-in forwards;
	-ms-animation: sharebnt 1s 0s 10000 ease-in forwards;
	
	-ms-transform-origin: right top;
	-webkit-transform-origin: right top;
	-o-transform-origin: right top;
	transform-origin: right top;
}

@keyframes sharebnt
{
	5%{
		transform:rotate(0deg); 
	}
	10%{
		transform:rotate(10deg); 
	}
	15%{
		transform:rotate(0deg);
	}
	20%{
		transform:rotate(-10deg);
	}
	25%{
		transform:rotate(0deg);
	}
	100%{
		transform:rotate(0deg);
	}
}

@-webkit-keyframes sharebnt
{
	5%{
		-webkit-transform:rotate(0deg); 
	}
	10%{
		-webkit-transform:rotate(10deg); 
	}
	15%{
		-webkit-transform:rotate(0deg);
	}
	20%{
		-webkit-transform:rotate(-10deg);
	}
	25%{
		-webkit-transform:rotate(0deg);
	}
	100%{
		-webkit-transform:rotate(0deg);
	}
}

@-o-keyframes sharebnt
{
	5%{
		-o-transform:rotate(0deg); 
	}
	10%{
		-o-transform:rotate(10deg); 
	}
	15%{
		-o-transform:rotate(0deg);
	}
	20%{
		-o-transform:rotate(-10deg);
	}
	25%{
		-o-transform:rotate(0deg);
	}
	100%{
		-o-transform:rotate(0deg);
	}
}

@-ms-keyframes sharebnt
{
	5%{
		-ms-transform:rotate(0deg); 
	}
	10%{
		-ms-transform:rotate(10deg); 
	}
	15%{
		-ms-transform:rotate(0deg);
	}
	20%{
		-ms-transform:rotate(-10deg);
	}
	25%{
		-ms-transform:rotate(0deg);
	}
	100%{
		-ms-transform:rotate(0deg);
	}
}

