}

.index-text{
	width: 90%;
	margin: 0 auto;
	font-size: 12px;
	text-align: left;
	color: #333333;
	line-height: 1.5em;
	padding-top: 10px;
}

.index-text a{
	display: inline-block;
	white-space: nowrap;
	color: #2ea4ff;
	text-decoration: underline;
}


.index-bnt{
	padding-top: 30px;
	text-align: center;
}

.logo-wrap{
	padding-top: 30px;
}

.index-bnt:nth-child(2){
